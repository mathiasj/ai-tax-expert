# AI / Embeddings
OPENAI_API_KEY=sk-...
COHERE_API_KEY=...

# LLM Provider (openai or anthropic)
LLM_PROVIDER=openai
ANTHROPIC_API_KEY=sk-ant-...
# LLM_MODEL=              # Override default model (gpt-4o / claude-sonnet-4-5-20250929)
LLM_MAX_TOKENS=4096
LLM_TEMPERATURE=0.1

# Auth
JWT_SECRET=change-me-to-a-random-string-at-least-32-chars
JWT_EXPIRES_IN=7d

# Cache
CACHE_TTL_SECONDS=3600

# Rate limiting (requests per minute)
RATE_LIMIT_ANONYMOUS=10
RATE_LIMIT_AUTHENTICATED=60

# Scraping (Firecrawl â€” needed for Skatteverket www4 WAF bypass)
FIRECRAWL_API_KEY=fc-...

# RAG config
RERANKER_TOP_N=5
RAG_TOP_K=20
RAG_TOKEN_BUDGET=6000

# Qdrant Vector DB
QDRANT_HOST=localhost
QDRANT_DB_PORT=6333
QDRANT_GRPC_PORT=6334
QDRANT_COLLECTION=tax_documents
# QDRANT_URL=             # Optional override (derived from QDRANT_HOST:QDRANT_DB_PORT)

# PostgreSQL
POSTGRES_HOST=localhost
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB_NAME=tax_expert
POSTGRES_DB_PORT=5432
# DATABASE_URL=            # Optional override (derived from components above)

# Redis (BullMQ)
REDIS_HOST=localhost
REDIS_CACHE_PORT=6379
# REDIS_URL=               # Optional override (derived from REDIS_HOST:REDIS_CACHE_PORT)

# Server
BACKEND_API_PORT=3050
APP_FRONTEND_PORT=5100
NODE_ENV=development
LOG_LEVEL=debug
