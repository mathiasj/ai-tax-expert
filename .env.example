# AI / Embeddings
OPENAI_API_KEY=sk-...
COHERE_API_KEY=...

# LLM Provider (openai or anthropic)
LLM_PROVIDER=openai
ANTHROPIC_API_KEY=sk-ant-...
# LLM_MODEL=              # Override default model (gpt-4o / claude-sonnet-4-5-20250929)
LLM_MAX_TOKENS=4096
LLM_TEMPERATURE=0.1

# Auth
JWT_SECRET=change-me-to-a-random-string-at-least-32-chars
JWT_EXPIRES_IN=7d

# Cache
CACHE_TTL_SECONDS=3600

# Rate limiting (requests per minute)
RATE_LIMIT_ANONYMOUS=10
RATE_LIMIT_AUTHENTICATED=60

# RAG config
RERANKER_TOP_N=5
RAG_TOP_K=20
RAG_TOKEN_BUDGET=6000

# Qdrant Vector DB
QDRANT_URL=http://localhost:6333
QDRANT_COLLECTION=tax_documents

# PostgreSQL
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/tax_expert

# Redis (BullMQ)
REDIS_URL=redis://localhost:6379

# Server
PORT=3000
NODE_ENV=development
LOG_LEVEL=debug
